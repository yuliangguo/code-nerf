{
  "arch": "codenerf",
  "net_hyperparams": {
    "shape_blocks": 3,
    "texture_blocks": 1,
    "W": 256,
    "num_xyz_freq": 10,
    "num_dir_freq": 4,
    "latent_dim": 256
  },
  "n_rays": 1024,
  "n_samples": 64,
  "roi_margin": 5,
  "loss_reg_coef": 1e-3,
  "loss_occ_coef": 1e-1,
  "loss_sym_coef": 1.0,
  "loss_obj_sz_coef": 1.0,
  "lr_schedule": [
    {"type":  "step", "lr":  1e-4, "interval":  250000},
    {"type":  "step", "lr":  1e-4, "interval":  250000}
  ],
  "shapenet_obj_cood": 1,
  "sym_aug": 0,
  "obj_sz_reg": 0,
  "euler_rot": 0,
  "optimize": {
    "num_opts": 100,
    "lr_shape": 0.01,
    "lr_texture": 0.02,
    "lr_pose": 0.01,
    "lr_half_interval": 1000
  }
